      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010214x/f1.highres" n="1"/>
      <p rend="incipit">14/3/79 ordre du jour</p>
      <p rend="incipit">Présents : <persName ref="#gp">Georges Perec</persName>. <persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName>. <persName ref="#le"><choice><abbr>LE</abbr><expan>Luc Etienne</expan></choice></persName>. <persName ref="#cb"><choice><abbr>CB</abbr><expan>Claude Berge</expan></choice></persName>. <persName ref="#jq"><choice><abbr>JQ</abbr><expan>Jean Queval</expan></choice></persName>. <persName ref="#jd"><choice><abbr>JD</abbr><expan>Jacques Duchateau</expan></choice></persName>. <persName ref="#jr"><choice><abbr>JR</abbr><expan>Jacques Roubaud</expan></choice></persName>. <persName ref="#na"><choice><abbr>NA</abbr><expan>Noël Arnaud</expan></choice></persName>. <persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName>. <persName ref="#pb"><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName>.</p>
      <p rend="incipit">Excusés : <persName ref="#fll"><choice><abbr>FLL</abbr><expan>François Le Lionnais</expan></choice></persName>. <persName ref="#cb"><choice><abbr>CB</abbr><expan>Claude Berge</expan></choice></persName>. <persName ref="#hm"><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName>. <persName>BL</persName>. <persName ref="#jb"><choice><abbr>JB</abbr><expan>Jacques Bens</expan></choice></persName>.</p>
      <p rend="incipit">Absents : les autres</p>
      <list type="ordered">
        <item>Création. <persName ref="#jd"><choice><abbr>JD</abbr><expan>Jacques Duchateau</expan></choice></persName>. <term>Chronologie</term>. <persName ref="#pb"><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName>. <term>Cryptobole</term></item>
        <item><persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName>. Au secours. Phynances. Photo Oulipo. <persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName>. <persName ref="#pb"><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName>.</item>
        <item><title>Bibliothèque oulipienne</title>. <persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName>. Manuscrits.</item>
        <item><title>Vol. II</title>. <persName ref="#jq"><choice><abbr>JQ</abbr><expan>Jean Queval</expan></choice></persName>. <persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName>.</item>
        <item>Sous-commission théâtre. <persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName>. <note n="1" resp="editor" xml:id="ftn1">Le numéro 5, écrit sous le numéro 8, a été remis à sa place dans la liste.</note></item>
        <item>Erudition. <persName ref="#na"><choice><abbr>NA</abbr><expan>Noël Arnaud</expan></choice></persName>. <persName ref="#cb"><choice><abbr>CB</abbr><expan>Claude Berge</expan></choice></persName>. <persName ref="#jd"><choice><abbr>JD</abbr><expan>Jacques Duchateau</expan></choice></persName> (<persName ref="#rq">Queneau</persName>). <persName ref="#pb"><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName>. <persName ref="#pb"><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName>.</item>
        <item>Menus propos. <persName ref="#gp"><choice><abbr>GP</abbr><expan>Georges Perec</expan></choice></persName>. <persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName>. <persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName>.</item>
        <item>Invités. <persName ref="#gp"><choice><abbr>GP</abbr><expan>Georges Perec</expan></choice></persName>. <persName ref="#cb"><choice><abbr>CB</abbr><expan>Claude Berge</expan></choice></persName>. <persName ref="#le"><choice><abbr>LE</abbr><expan>Luc Etienne</expan></choice></persName>.</item>
        <item>Nouvelle réunion.</item>
      </list>
