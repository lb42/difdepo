      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010198n/f3.highres" n="3"/>
      <p rend="incipit">25-8-77 Ordre du jour</p>
      <p><label type="rubrique">Création =</label></p>
      <list type="unordered">
        <item>1 <persName>Perec</persName> &gt; Hommage à l’oulipo, Work in progress, <persName>Plassard</persName></item>
        <item><persName>Fournel</persName>+<persName><choice><abbr>JR</abbr><expan>Jacques Roubaud</expan></choice></persName> &gt; le <term>cylindre</term> ou <title>l’hôtel de Sens</title></item>
        <item><persName>Mathews</persName> &gt; conséquence de l’<term>algorithme</term></item>
        <item><persName>Bénabou</persName> &gt; <unclear>***</unclear> suggestions</item>
        <item><persName><choice><abbr>FLL</abbr><expan>François Le Lionnais</expan></choice></persName> &gt; lettre à <persName>Billy</persName></item>
        <item><persName>Bens</persName> &gt; <persName>Charles</persName> au pied de la lettre <note n="1" resp="editor" xml:id="ftn1">Page entièrement barrée</note></item>
      </list>
      <p><label type="rubrique">Action</label> </p>
      <p><persName><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName> <name>Avignon</name>- Photo- <title>Volume 2</title> <orgName>CNL</orgName>- <title>Drôle de baroque</title></p>
      <p><title>Bibliothèque oulipienne</title></p>
      <list type="unordered">
        <item><persName><choice><abbr>FLL</abbr><expan>François Le Lionnais</expan></choice></persName> Fin du <title>volume Queneau</title></item>
        <item>Paiement</item>
        <item>volume sous presse</item>
      </list>
      <p><label type="rubrique">Phynances</label></p>
      <list type="unordered">
        <item><orgName>Caisse d’Epargne</orgName></item>
        <item>Phynances secrétariat pour <persName>Fournel</persName> (spécial <name>Avignon</name>)</item>
      </list>
      <p><label type="rubrique">Menu Propos</label></p>
      <list type="ordered">
        <item><name>Ordinateur et écrivain</name> <persName><choice><abbr>JB</abbr><expan>Jacques Bens</expan></choice></persName> <orgName>France Culture</orgName>.</item>
        <item><title>Ecrire</title> de <persName>Jean Guénot</persName> <persName><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> <term>Collages</term></item>
        <item><persName>Laclos</persName> <persName><choice><abbr>LE</abbr><expan>Luc Etienne</expan></choice></persName> <title>Charlie Hebdo</title>.</item>
      </list>
      <p><persName>Queneau</persName> <persName><choice><abbr>FLL</abbr><expan>François Le Lionnais</expan></choice></persName> Des nouvelles de la succession <persName>Queneau</persName>.</p>
      <p><persName><choice><abbr>LE</abbr><expan>Luc Etienne</expan></choice></persName> Spectacle <persName>Queneau</persName></p>
      <p><note n="2" resp="editor" xml:id="ftn2">Page entièrement barrée</note></p>
