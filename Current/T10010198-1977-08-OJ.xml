      <list type="unordered">
        <item><orgName>Caisse d’Epargne</orgName></item>
        <item>Phynances secrétariat pour <persName ref="#pf">Fournel</persName> (spécial <name type="manif">Avignon</name>)</item>
      </list>
      <p><label type="rubrique">Menu Propos</label></p>
      <list type="ordered">
        <item><name type="manif">Ordinateur et écrivain</name> <persName ref="#jb">JB</persName> <orgName>France Culture</orgName>.</item>
        <item><title>Ecrire</title> de <persName>Jean Guénot</persName> <persName>MB</persName> <term>Collages</term></item>
        <item><persName>Laclos</persName> <persName>LE</persName> <title>Charlie Hebdo</title>.</item>
