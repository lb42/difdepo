      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010198n/f3.highres" n="3"/>
      <p rend="incipit">25-8-77 Ordre du jour</p>
      <p><label type="rubrique">Création =</label></p>
      <list type="unordered">
        <item>1 <persName ref="#gp">Perec</persName> &gt; Hommage à l’oulipo, Work in progress, <persName>Plassard</persName></item>
        <item><persName ref="#pf">Fournel</persName>+<persName ref="#jr">JR</persName> &gt; le <term>cylindre</term> ou <title>l’hôtel de Sens</title></item>
        <item><persName ref="#hm">Mathews</persName> &gt; conséquence de l’<term>algorithme</term></item>
        <item><persName ref="#mb">Bénabou</persName> &gt; <unclear>***</unclear> suggestions</item>
        <item><persName ref="#fll">FLL</persName> &gt; lettre à <persName>Billy</persName></item>
        <item><persName ref="#jb">Bens</persName> &gt; <persName>Charles</persName> au pied de la lettre <note n="1" resp="editor" xml:id="ftn1">Page entièrement barrée</note></item>
      </list>
      <p><label type="rubrique">Action</label> </p>
      <p><persName ref="#pf">PF</persName> <name type="manif">Avignon</name>- Photo- <title>Volume 2</title> <orgName>CNL</orgName>- <title>Drôle de baroque</title></p>
      <p><title>Bibliothèque oulipienne</title></p>
      <list type="unordered">
        <item><persName ref="#fll">FLL</persName> Fin du <title>volume Queneau</title></item>
        <item>Paiement</item>
        <item>volume sous presse</item>
      </list>
