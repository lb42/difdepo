      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010283c/f4.highres" n="4"/>
      <p rend="incipit">Oulipo 08-06-85</p>
      <p rend="incipit">Présents : <persName>Paul Braffort</persName> président, <persName>Jacques Jouet</persName> secrétaire, <persName>Jacques Roubaud</persName>, <persName>François Caradec</persName>, <persName>Harry Mathews</persName>, <persName>Jacques Bens</persName> (hôte), <persName>Paul Fournel</persName>, <persName>Marcel Bénabou</persName></p>
      <p rend="incipit">Excusés : <persName>Michèle Métail</persName>, <persName>Jean Queval</persName>, <persName>Claude Berge</persName>, <persName>Pierre Rosenstiehl</persName>, <persName>Italo Calvino</persName>, <persName>Noël Arnaud</persName></p>
      <p rend="incipit">Inexcusés : <persName>Jean Lescure</persName>, <persName>Jacques Duchateau</persName></p>
      <p><label type="rubrique">1 Création</label></p>
      <list type="unordered">
        <item><persName><choice><abbr>PR</abbr><expan>Pierre Rosenstiehl</expan></choice></persName> (<persName><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName>)</item>
        <item><persName><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName> Technique poétique <persName><choice><abbr>FLL</abbr><expan>François Le Lionnais</expan></choice></persName></item>
        <item><persName><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> Inutilité de la rime</item>
      </list>
      <p><label type="rubrique">2 Action</label></p>
      <list type="unordered">
        <item><persName><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> le point sur le guide</item>
        <item><persName><choice><abbr>JB</abbr><expan>Jacques Bens</expan></choice></persName> <name type="manif">Créteil</name></item>
        <item><persName><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> <persName><choice><abbr>GP</abbr><expan>Georges Perec</expan></choice></persName></item>
        <item><persName><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> Vittoria</item>
        <item><persName><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName> réunion avec <name type="manif">Oupeinpo</name></item>
        <item><persName><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName> <orgName>Invisible Seattle</orgName></item>
      </list>
      <p><label type="rubrique">3 Adresse du président</label> +</p>
      <p><label type="rubrique">4 <title>Bibliothèque oulipienne</title></label> + <persName><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName></p>
      <p><label type="rubrique">5 Erudition</label></p>
      <list type="unordered">
        <item><persName><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> <persName>Pierre de Saint Lois</persName></item>
        <item><persName><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> <term>Canada Dry</term> <persName><choice><abbr>AB</abbr><expan>André Blavier</expan></choice></persName></item>
        <item><persName><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> <persName>François le Gloupier</persName></item>
        <item><persName><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName> <term>Erreur</term> et <term>clinamen</term></item>
      </list>
      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010283c/f5.highres" n="5"/>
      <p><label type="rubrique">6 Menus propos</label></p>
      <list type="unordered">
        <item><persName><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> Photos du <name type="manif">Congrès</name></item>
        <item><persName><choice><abbr>JB</abbr><expan>Jacques Bens</expan></choice></persName> Oulipo France Musique</item>
      </list>
