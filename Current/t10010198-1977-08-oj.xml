      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010198n/f3.highres" n="3"/>
      <p rend="incipit">25-8-77 Ordre du jour</p>
      <p><label type="rubrique">Création =</label></p>
      <list type="unordered">
        <item>1 <persName ref="#gp">Perec</persName> &gt; Hommage à l’oulipo, Work in progress, <persName ref="#plassard">Plassard</persName></item>
        <item><persName ref="#pf">Fournel</persName>+<persName ref="#jr"><abbr>JR</abbr></persName> &gt; le <term ref="#cylindre">cylindre</term> ou <title ref="#lhoteldesens">l’hôtel de Sens</title></item>
        <item><persName ref="#hm">Mathews</persName> &gt; conséquence de l’<term ref="#algorithme">algorithme</term></item>
        <item><persName ref="#mb">Bénabou</persName> &gt; <unclear>***</unclear> suggestions</item>
        <item><persName ref="#fll"><abbr>FLL</abbr></persName> &gt; lettre à <persName ref="#billy">Billy</persName></item>
        <item><persName ref="#jb">Bens</persName> &gt; <persName ref="#charles">Charles</persName> au pied de la lettre <note n="1" resp="editor">Page entièrement barrée</note></item>
      </list>
      <p><label type="rubrique">Action</label> </p>
      <p><persName ref="#pf"><abbr>PF</abbr></persName> <name ref="#avignon" type="manif">Avignon</name>- Photo- <title ref="#volume">Volume 2</title> <orgName ref="#cnl">CNL</orgName>- <title ref="#droledebaroque">Drôle de baroque</title></p>
      <p><title ref="#bibliothequeoulipienne">Bibliothèque oulipienne</title></p>
      <list type="unordered">
        <item><persName ref="#fll"><abbr>FLL</abbr></persName> Fin du <title ref="#volumequeneau">volume Queneau</title></item>
        <item>Paiement</item>
        <item>volume sous presse</item>
      </list>
      <p><label type="rubrique">Phynances</label></p>
      <list type="unordered">
        <item><orgName ref="#caissedepargne">Caisse d’Epargne</orgName></item>
        <item>Phynances secrétariat pour <persName ref="#pf">Fournel</persName> (spécial <name ref="#avignon" type="manif">Avignon</name>)</item>
      </list>
      <p><label type="rubrique">Menu Propos</label></p>
      <list type="ordered">
        <item><name ref="#ordinateuretecrivain" type="manif">Ordinateur et écrivain</name> <persName ref="#jb"><abbr>JB</abbr></persName> <orgName ref="#franceculture">France Culture</orgName>.</item>
        <item><title ref="#ecrire">Ecrire</title> de <persName ref="#jeanguenot">Jean Guénot</persName> <persName ref="#mb"><abbr>MB</abbr></persName> <term ref="#collages">Collages</term></item>
        <item><persName ref="#laclos">Laclos</persName> <persName ref="#le"><abbr>LE</abbr></persName> <title ref="#charliehebdo">Charlie Hebdo</title>.</item>
      </list>
      <p><persName ref="#rq">Queneau</persName> <persName ref="#fll"><abbr>FLL</abbr></persName> Des nouvelles de la succession <persName ref="#rq">Queneau</persName>.</p>
      <p><persName ref="#le"><abbr>LE</abbr></persName> Spectacle <persName ref="#rq">Queneau</persName></p>
      <p><note n="2" resp="editor">Page entièrement barrée</note></p>
