      <p rend="incipit">11.12.81. chez <persName ref="#pb">Braffort</persName>.</p>
      <p rend="incipit">Présents.</p>
      <list type="unordered">
        <item><persName ref="#jl">Lescure</persName></item>
        <item><persName ref="#jr">Roubaud</persName></item>
        <item><persName ref="#pb">Braffort</persName></item>
        <item><persName ref="#pf">Fournel</persName></item>
        <item><persName ref="#mm">Metail</persName>.</item>
      </list>
      <p rend="incipit">Excusés.</p>
      <list type="unordered">
        <item><persName ref="#jb">Bens</persName></item>
        <item><persName ref="#gp">Perec</persName></item>
        <item><persName ref="#jd">Duchâteau</persName></item>
        <item><persName ref="#el">Etienne</persName></item>
        <item><persName ref="#jq">Queval</persName>.</item>
        <item><persName>Le Lionnais</persName></item>
