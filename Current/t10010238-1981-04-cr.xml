        <persName ref="difdepo:persons.xml#jl">Lescure</persName>
        <persName ref="difdepo:persons.xml#mm">Métail</persName>
        <persName ref="difdepo:persons.xml#hm">Mathews</persName>
        <persName ref="difdepo:persons.xml#gp">Perec</persName>
        <persName ref="difdepo:persons.xml#queneau">Queneau</persName>
        <persName ref="difdepo:persons.xml#jq">Queval</persName>
        <persName ref="difdepo:persons.xml#jr">Roubaud</persName>
        <persName ref="difdepo:persons.xml#ams">Schmidt</persName>
      </p>
      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010238j/f6.highres" n="6"/>
      <p>
        <label type="rubrique">6. Erudition</label>
      </p>
      <p><persName ref="difdepo:persons.xml#jd"><abbr>JD</abbr></persName> Livre de <persName ref="difdepo:persons.xml#todorov">Todorov</persName> et <persName ref="difdepo:persons.xml#bakhtine">Bakhtine</persName></p>
      <p>
        <persName ref="difdepo:persons.xml#mb">
          <abbr>MB</abbr>
        </persName>
        <persName ref="difdepo:persons.xml#kennethkoch">Kenneth Koch</persName>
        <persName ref="difdepo:persons.xml#jabigoudy">Jabi Goudy</persName>
      </p>
      <p>
        <label type="rubrique">7. Menus propos</label>
      </p>
      <p><name ref="difdepo:events.xml#prochainereunion" type="manif">Prochaine réunion</name>.</p>
