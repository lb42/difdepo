        <persName ref="difdepo:persons.xml#ic">Calvino</persName>
        <persName ref="difdepo:persons.xml#sc">Chapman</persName>
        <persName ref="difdepo:persons.xml#rc">Chambers</persName>
        <persName ref="difdepo:persons.xml#md">Duchamp</persName>
        <persName ref="difdepo:persons.xml#jd">Duchateau</persName>
        <persName ref="difdepo:persons.xml#le">Etienne</persName>
