            <persName ref="difdepo:persons.xml#roubaud">Roubaud</persName> 
            <persName ref="difdepo:persons.xml#schmidt">Schmidt</persName>
         </p>
         <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010238j/f6.highres" n="6"/>
