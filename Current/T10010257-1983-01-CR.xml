      <p>4 <persName>Pouilloux</persName> / <persName ref="#mb">Bénabou</persName></p>
      <p>6 <persName>Magné</persName> / <persName ref="#mb">Bénabou</persName></p>
      <p>5 <persName>Devos</persName> / <persName ref="#pb">P. Braffort</persName></p>
      <p><persName>J</persName> et <persName ref="#bc">B. Cerquiglini</persName>, <persName>Nadaud</persName>, <persName>Zumthor</persName></p>
