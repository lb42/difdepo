        <item>14 = 15</item>
      </list>
      <p><persName ref="difdepo:persons.xml#fc"><abbr>FC</abbr></persName></p>
      <list type="unordered">
        <item>Chaîne</item>
      </list>
      <p><persName ref="difdepo:persons.xml#mb"><abbr>MB</abbr></persName> – <title ref="difdepo:titles.xml#pourquoi">Pourquoi</title></p>
      <p><persName ref="difdepo:persons.xml#pb"><abbr>PB</abbr></persName> – <persName ref="difdepo:persons.xml#robertneverroust">Robert né Verroust</persName> (<term ref="difdepo:terms.xml#pangrammeminimal">Pangramme minimal</term>)</p>
