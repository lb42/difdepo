      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b100103272/f1.highres" n="1"/>
      <p rend="incipit">Oulipo</p>
      <p>26 mai 1989</p>
      <p>Excusé : <persName ref="#jr">JR</persName></p>
      <p>Présents <persName>HM</persName> (Président) <persName ref="#jj">JJ</persName> (Secrétaire)</p>
      <p><persName ref="#mm">MM</persName></p>
      <p><persName ref="#cb">CB</persName></p>
      <p><persName ref="#na">NA</persName></p>
      <p><persName ref="#fc">FC</persName></p>
      <p><persName ref="#pf">PF</persName></p>
