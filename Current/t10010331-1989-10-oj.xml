      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010331s/f1.highres" n="1"/>
      <p rend="incipit">17 octobre 327e séance</p>
      <p rend="incipit">Président <persName><abbr>JR</abbr></persName> secrétaire <persName><abbr>MB</abbr></persName></p>
      <p rend="incipit">Invité d’honneur, <persName>G. Lascault</persName></p>
      <list type="ordered">
        <item>Présents <persName><abbr>CB</abbr></persName>, <persName><abbr>JJ</abbr></persName>, <persName><abbr>FC</abbr></persName>, <persName><abbr>MB</abbr></persName>, <persName><abbr>HM</abbr></persName>, <persName><abbr>MM</abbr></persName>, <persName><abbr>PF</abbr></persName>, <persName><abbr>JR</abbr></persName></item>
      </list>
      <p rend="incipit">excusés <persName><abbr>NA</abbr></persName>, <persName><abbr>PR</abbr></persName>, <persName><abbr>JL</abbr></persName>, <persName><abbr>JD</abbr></persName></p>
      <p rend="incipit">absents <persName><abbr>PB</abbr></persName>, <persName><abbr>JQ</abbr></persName>, <persName><abbr>JB</abbr></persName>,</p>
      <list type="ordered">
        <item>Présentation par <persName><abbr>PF</abbr></persName></item>
        <item>création</item>
        <item><persName><abbr>HM</abbr></persName> <title>le journaliste</title></item>
        <item><persName><abbr>JJ</abbr></persName> <title>un papillon rarissime</title></item>
        <item><persName><abbr>MM</abbr></persName> <title>50 poèmes oligogrammes</title></item>
      </list>
      <p><label type="rubrique">Erudition</label></p>
      <list type="ordered">
        <item><persName><abbr>MM</abbr></persName> <hi rend="sup">1</hi> <term>palindrome</term></item>
        <item><persName><abbr>MM</abbr></persName> <hi rend="sup">2</hi> <term>rébus</term> chinois</item>
        <item><persName><abbr>HM</abbr></persName> cherokee</item>
        <item><persName><abbr>CB</abbr></persName> <hi rend="sup">1</hi> <persName>Albert Camus</persName> ?</item>
        <item><persName><abbr>CB</abbr></persName> <hi rend="sup">2</hi> dictionnaire des <term>anagrammes</term></item>
        <item><persName><abbr>JR</abbr></persName> peri oulipisme allemand <persName>Pastior</persName>, <persName>Factor</persName></item>
        <item><persName><abbr>MB</abbr></persName> appel pressant</item>
        <item><persName><abbr>FC</abbr></persName> <title>de re…</title></item>
      </list>
      <p><label type="rubrique">Action</label></p>
      <list type="ordered">
        <item><persName><abbr>MM</abbr></persName> fouji</item>
        <item><persName><abbr>MB</abbr></persName> <hi rend="sup">1</hi> protestation véhémente 16, 16 bis. <persName><abbr>MB</abbr></persName> <hi rend="sup">2</hi> <ref>Archives</ref></item>
        <item><persName><abbr>PF</abbr></persName> <hi rend="sup">1</hi> Ivry</item>
        <item><persName><abbr>PF</abbr></persName> <hi rend="sup">2</hi> Allemagne</item>
        <item><persName><abbr>PF</abbr></persName> <hi rend="sup">3</hi> Mexico</item>
        <item><persName><abbr>FC</abbr></persName> qu’est-ce que l’oulipo ?</item>
      </list>
      <p>Ce n’est pas une école c’est une crèche</p>
      <p><title>Bibliothèque oulipienne</title></p>
      <list type="ordered">
        <item><persName><abbr>PF</abbr></persName> <title>Bibliothèque oulipienne</title> III</item>
        <item><persName><abbr>JJ</abbr></persName> état</item>
        <item><persName><abbr>FC</abbr></persName> + oulipo dans le Monde - polyglotte?</item>
      </list>
      <list type="ordered">
        <item><persName><abbr>MM</abbr></persName> porte 29. <persName><abbr>HM</abbr></persName> <orgName>WICE</orgName></item>
        <item><persName><abbr>PF</abbr></persName> <unclear>*</unclear> <persName><abbr>MB</abbr></persName> <title>bibliothèque</title></item>
        <item><persName><abbr>JJ</abbr></persName> Louvre 30. Invités d’honneur 31. <name type="manif">prochaine réunion</name></item>
        <item><persName>FC Bocquillon</persName></item>
      </list>
