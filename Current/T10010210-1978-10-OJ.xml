      <p><label type="rubrique">CRÉATION</label></p>
      <list type="unordered">
        <item><title>volume 2</title> <persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> +<persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName></item>
