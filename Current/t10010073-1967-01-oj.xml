        <item><term>Plagiats par anticipation</term> (PLAPA) <term>themersoquenellien</term>) (<persName>LATIS</persName>)</item>
        <item><term>Bibliographie</term> : <persName>Prieto</persName> : « <title>Messages et Signaux</title> » (<orgName>P.U.F.</orgName>) (<persName><abbr>F.L.L.</abbr></persName>)</item>
        <item><term>Littérature pythagorienne</term> (<persName>Sevy Frecel</persName>) (<persName>C. BERGE</persName>)</item>
