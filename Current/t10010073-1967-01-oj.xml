        <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b100100736/f3.highres" n="3"/>
        <item><add><term>Fatalité séquentielle</term> (<persName><abbr>J.R.</abbr></persName>)</add> <note n="9" resp="editor">Ajouté à la main</note></item>
