      <p rend="incipit">du 23-8-85</p>
      <p rend="incipit">Présents <persName><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName>, <persName><choice><abbr>JB</abbr><expan>Jacques Bens</expan></choice></persName>, <persName><choice><abbr>JJ</abbr><expan>Jacques Jouet</expan></choice></persName>, <persName><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName>, <persName><choice><abbr>JR</abbr><expan>Jacques Roubaud</expan></choice></persName>, <persName><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName>, <persName><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> + <persName><choice><abbr>CB</abbr><expan>Claude Berge</expan></choice></persName> (en retard)</p>
