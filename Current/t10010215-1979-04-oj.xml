      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010215c/f1.highres" n="1"/>
      <p rend="incipit">Réunion du 24 Avril 79 <lb/>chez Dominique Roussillon</p>
      <list type="unordered">
        <item>Présents : <persName><abbr>FLL</abbr></persName>, <persName><abbr>LE</abbr></persName>, <persName><abbr>JQ</abbr></persName>, <persName><abbr>CB</abbr></persName>, <persName><abbr>MB</abbr></persName>, <persName><abbr>PF</abbr></persName>, <persName><abbr>PB</abbr></persName>.</item>
        <item>Excusés : <persName>Blavier</persName>, <persName><abbr>NA</abbr></persName>.</item>
        <item>Absents: <persName><abbr>GP</abbr></persName>, <persName><abbr>JR</abbr></persName>, <persName><abbr>JL</abbr></persName>, <persName><abbr>IC</abbr></persName></item>
        <item>Président: <persName><abbr>PF</abbr></persName></item>
        <item>Invités: <persName>J</persName> et <persName>A Rosentiehl</persName></item>
      </list>
      <p>ORDRE DU JOUR</p>
      <p><label type="rubrique">Création</label></p>
      <list type="unordered">
        <item>Suggestion de <unclear>C</unclear>. <persName><abbr>MB</abbr></persName></item>
        <item>mini suggestion <persName><abbr>CB</abbr></persName></item>
        <item><persName><abbr>FLL</abbr></persName> pour <persName><abbr>JL</abbr></persName>: Exercice oulipien.</item>
        <item>Microcréation de <persName>Rosenstiehl</persName></item>
        <item><persName>Noël Arnaud</persName> <persName><abbr>PF</abbr></persName></item>
        <item>Invités <persName><abbr>CB</abbr></persName></item>
      </list>
      <p><label type="rubrique">Action</label></p>
      <list type="unordered">
        <item><orgName>CNL</orgName>: <persName><abbr>PF</abbr></persName> +<persName><abbr>PF</abbr></persName>: volume 2</item>
        <item><title>Bibliothèque oulipienne</title>: <persName><abbr>PF</abbr></persName></item>
        <item>Phynances: <persName><abbr>MB</abbr></persName></item>
        <item>Carte postale <persName><abbr>FLL</abbr></persName></item>
        <item><persName><abbr>FLL</abbr></persName>: Chevalet</item>
        <item><persName><abbr>PF</abbr></persName>: <persName>Balpe</persName></item>
      </list>
      <p><label type="rubrique">Menus Propos</label></p>
      <list type="unordered">
        <item><persName><abbr>FLL</abbr></persName>: convalescence oulipienne</item>
        <item><persName><abbr>MB</abbr></persName>: projet de vitrine</item>
      </list>
      <p><name type="manif">Prochaine réunion</name> : <lb/>Invités <lb/>17 mai; jeudi 14 juin</p>
