      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b100102801/f2.highres" n="2"/>
      <p rend="incipit">Ordre du jour</p>
      <p><label type="rubrique">Propos d’Apéro</label></p>
      <p><persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName> : <persName ref="#ic">Italo Calvino</persName></p>
      <p><persName ref="#jb"><choice><abbr>JB</abbr><expan>Jacques Bens</expan></choice></persName> : <persName>Emile Lessafre</persName></p>
      <p><persName ref="#pb"><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName> : Immatériaux</p>
      <p><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> : Don du Quebec</p>
      <p><label type="rubrique">Création</label></p>
      <p><persName ref="#hm"><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName> : le duel littéraire</p>
      <p><persName ref="#pb"><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName> : Projet de <persName>Paulette Perec</persName></p>
      <p>Collectif <persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> : guide de littérature</p>
      <p><persName ref="#jb"><choice><abbr>JB</abbr><expan>Jacques Bens</expan></choice></persName> : épouvantail</p>
      <p><persName ref="#fc"><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> : l’être supérieur</p>
      <p><label type="rubrique">Action</label></p>
      <p><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> : <name type="manif">Villeneuve</name></p>
      <p><persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName> : <title>Bibliothèque oulipienne</title></p>
      <p><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> : <name type="manif">Limoges</name> + <persName ref="#hm"><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName></p>
      <p><persName ref="#jb"><choice><abbr>JB</abbr><expan>Jacques Bens</expan></choice></persName> : Prospective Pavic</p>
      <p><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> : Espagne</p>
      <p><label type="rubrique">Erudition</label></p>
      <p><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> : <term>plagiaire</term></p>
      <p><persName ref="#hm"><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName> : tireur à la ligne <term>poétique musical</term></p>
      <p><persName ref="#fc"><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> : - un ho de lettres</p>
      <list type="unordered">
        <item>une curieuse exposition</item>
        <item>Maboulite <term>holorimeux</term></item>
        <item><term>Tragédie</term> classique</item>
      </list>
      <p><persName ref="#pb"><choice><abbr>PB</abbr><expan>Paul Braffort</expan></choice></persName> :</p>
      <p><label type="rubrique">Menus propos</label></p>
      <p><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> : création proposée</p>
      <p><persName ref="#hm"><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName> : traduction de <title>La Vie Mode d’Emploi</title></p>
