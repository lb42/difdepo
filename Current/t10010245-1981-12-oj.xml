      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010245m/f1.highres" n="1"/>
      <p rend="incipit">Réunion du 11 XII 81 chez PB</p>
      <p rend="incipit">PF président</p>
      <p><label type="rubrique">Création :</label></p>
      <list type="unordered">
        <item><persName ref="#pf"><abbr>PF</abbr></persName> : Un absent de marque.</item>
        <item><persName ref="#jr"><abbr>JR</abbr></persName> : <persName ref="#giordanobruno">Giordano Bruno</persName> et l’<orgName ref="#oulipo">oulipo</orgName>.</item>
        <item><persName ref="#pb"><abbr>PB</abbr></persName> : <term ref="#romanpolicierplagie">Roman policier plagié</term>.</item>
      </list>
      <p><label type="rubrique">Action</label></p>
      <list type="unordered">
        <item><persName ref="#pf"><abbr>PF</abbr></persName> : <name ref="#nantes" type="manif">Nantes</name></item>
        <item><persName ref="#pf"><abbr>PF</abbr></persName> : <name ref="#villeneuve" type="manif">Villeneuve</name></item>
        <item>Bibliothèque oulipienne : <persName ref="#pf"><abbr>PF</abbr></persName></item>
        <item><persName ref="#pf"><abbr>PF</abbr></persName> + tous : <title ref="#atlas">Atlas</title></item>
        <item><persName ref="#pb"><abbr>PB</abbr></persName> : <orgName ref="#alamo">Alamo</orgName></item>
      </list>
      <p><label type="rubrique">Erudition.</label></p>
      <list type="unordered">
        <item><persName ref="#jl">J Lescure</persName> : AEIOU</item>
        <item><persName ref="#jl"><abbr>JL</abbr></persName> : le mystère en fleur <persName ref="#jacquescharpentreau">Jacques Charpentreau</persName></item>
        <item><persName ref="#pb"><abbr>PB</abbr></persName> : <title ref="#lacosmogonie">La cosmogonie</title> vue par <persName ref="#ic">Calvino</persName>.</item>
        <item><persName ref="#pb"><abbr>PB</abbr></persName> : <title ref="#dictionnairedelinformatique">dictionnaire de l’informatique</title></item>
        <item><persName ref="#pf"><abbr>PF</abbr></persName> <persName ref="#pb"><abbr>PB</abbr></persName> : <term ref="#jeudecartes">jeu de cartes</term></item>
      </list>
      <p><name ref="#prochainereunion" type="manif">Prochaine réunion</name>  : vendredi 22 janvier 82</p>
