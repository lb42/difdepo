      <list type="unordered">
        <item><persName ref="difdepo:persons.xml#pb"><abbr>PB</abbr></persName> : <persName ref="difdepo:persons.xml#sheldonklein">Sheldon Klein</persName></item>
        <item><persName ref="difdepo:persons.xml#le"><abbr>LE</abbr></persName></item>
