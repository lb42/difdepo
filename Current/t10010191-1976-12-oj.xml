        <item>Amérique (<persName ref="difdepo:persons.xml#hm"><abbr>HM</abbr></persName>)</item>
        <item>Une possible disciple (<persName ref="difdepo:persons.xml#mb"><abbr>MB</abbr></persName>) <persName ref="difdepo:persons.xml#annemariechristin">Anne-Marie Christin</persName>, <persName ref="difdepo:persons.xml#marionmilner">Marion Milner</persName></item>
