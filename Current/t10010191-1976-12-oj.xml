      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010191h/f30.highres" n="30"/>
      <p rend="incipit">20 Décembre 76 Président <persName ref="#pb">Braffort Paul</persName></p>
      <p rend="incipit">SÉANCE N°</p>
      <p rend="incipit">ORDRE DU JOUR</p>
      <p rend="incipit">Absents : <persName ref="#jl"><abbr>JL</abbr></persName>
        <persName ref="#ab"><abbr>AB</abbr></persName>
        <persName ref="#cb"><abbr>CB</abbr></persName>
        <persName ref="#mm"><abbr>MM</abbr></persName>
        <persName ref="#jd"><abbr>JD</abbr></persName>
        <persName ref="#ic"><abbr>IC</abbr></persName>
        <persName ref="#jr"><abbr>JR</abbr></persName></p>
      <p rend="incipit">Présents : <persName ref="#hm"><abbr>HM</abbr></persName>
        <persName ref="#gp"><abbr>GP</abbr></persName>
        <persName ref="#le"><abbr>LE</abbr></persName>
        <persName ref="#na"><abbr>NA</abbr></persName>
        <persName ref="#jq"><abbr>JQ</abbr></persName>
        <persName ref="#pf"><abbr>PF</abbr></persName>
        <persName ref="#pb"><abbr>PB</abbr></persName>
        <persName ref="#mb"><abbr>MB</abbr></persName>
        <persName ref="#jb"><abbr>JB</abbr></persName>
        <persName ref="#fll"><abbr>FLL</abbr></persName></p>
      <p><label type="rubrique">Action :</label></p>
      <list type="unordered">
        <item>Enveloppes (<persName ref="#pf"><abbr>PF</abbr></persName>
          <persName ref="#jb"><abbr>JB</abbr></persName>
          <persName ref="#na"><abbr>NA</abbr></persName>) prêt <orgName ref="#ibm">IBM</orgName></item>
        <item><name ref="#avignon" type="manif">Avignon</name> (<persName ref="#pf"><abbr>PF</abbr></persName>) <name ref="#semaineoulipo" type="manif">semaine OULIPO</name>
          (15.7-15.8)</item>
        <item><title ref="#bibliothequeoulipienne">Bibliothèque oulipienne</title> spécial <persName ref="#rq">Queneau</persName> (<persName ref="#gp"><abbr>GP</abbr></persName>+<persName ref="#pf"><abbr>PF</abbr></persName>)</item>
        <item>Amérique (<persName ref="#hm"><abbr>HM</abbr></persName>)</item>
        <item>Une possible disciple (<persName ref="#mb"><abbr>MB</abbr></persName>) <persName ref="#annemariechristin">Anne-Marie
            Christin</persName>, <persName ref="#marionmilner">Marion Milner</persName></item>
        <item>Grande photo oulipo (<persName ref="#fll"><abbr>FLL</abbr></persName>)</item>
      </list>
      <p><label type="rubrique">Administration :</label></p>
      <list type="unordered">
        <item>Statut légal (<persName ref="#pf"><abbr>PF</abbr></persName>)</item>
        <item>Phynance (secrétariat) (<persName ref="#fll"><abbr>FLL</abbr></persName>)</item>
      </list>
      <p><label type="rubrique">Créations :</label></p>
      <list type="unordered">
        <item>Dans la ligne détournée de <persName ref="#gp">Perec</persName> (<persName ref="#pf"><abbr>PF</abbr></persName>)</item>
        <item><term ref="#asla">ASLA</term> (<persName ref="#pb"><abbr>PB</abbr></persName>-<persName ref="#pf"><abbr>PF</abbr></persName>)</item>
        <item><term ref="#sautemouton">saute-mouton</term> (<persName ref="#jq"><abbr>JQ</abbr></persName>)</item>
      </list>
      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010191h/f31.highres" n="31"/>
      <p><label type="rubrique">Menus propos :</label></p>
      <list type="unordered">
        <item><persName ref="#ab">Blavier</persName> « <title ref="#occupetoidhomelies">occupe-toi
            d’homélies</title> » (<persName ref="#pb"><abbr>PB</abbr></persName>)</item>
        <item><persName ref="#jb"><abbr>JB</abbr></persName></item>
      </list>
      <p><title ref="#cahiersqueneau">Cahiers Queneau</title> : <persName ref="#jq"><abbr>JQ</abbr></persName>-<persName ref="#mb"><abbr>MB</abbr></persName>-<persName ref="#jb"><abbr>JB</abbr></persName></p>
      <p><label type="rubrique">Erudition :</label></p>
      <list type="unordered">
        <item><persName ref="#pb"><abbr>PB</abbr></persName> : <persName ref="#sheldonklein">Sheldon Klein</persName></item>
        <item><persName ref="#le"><abbr>LE</abbr></persName></item>
        <item><persName ref="#hm"><abbr>HM</abbr></persName></item>
      </list>
      <p>Après <persName ref="#rq"><abbr>RQ</abbr></persName> : <persName ref="#fll"><abbr>FLL</abbr></persName> « <title>Fronton</title> » dans la  <title ref="#nrf">Nouvelle Revue Française</title> de
        Février.</p>
      <p>Prochaine date (prochain invité)</p>
      <p>Jeudi 27/1 <persName ref="#cortada">Cortada</persName></p>
