  <text type="OJ">
    <body>
      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b10010207v/f1.highres" n="1"/>
      <p rend="incipit">Ordre du jour</p>
      <p rend="incipit">Le 27 juin 78</p>
      <p rend="incipit">à 18h chez G. Perec</p>
      <p><label type="rubrique">Action :</label></p>
      <list type="unordered">
        <item><persName ref="#mb">MB</persName> : Caisse</item>
        <item><persName ref="#mb">MB</persName> + PF : B. O. + Projets + libraires + nouvel imprimeur.</item>
        <item><persName ref="#pf">PF</persName> : Avignon → JB</item>
        <item><persName ref="#pf">PF</persName> : Royaumont</item>
        <item><persName ref="#pf">PF</persName> : <persName ref="#mm">Métail</persName></item>
      </list>
      <p><label type="rubrique">Création :</label></p>
      <list type="unordered">
        <item><persName ref="#pf">PF</persName> : <persName ref="#jr">Roubaud</persName> absent → Hôtel de Sens parti</item>
        <item><persName ref="#gp">GP</persName> : Projet <title>« trompe-l’œil »</title></item>
        <item><persName ref="#jb">JB</persName> : Projet : <title>« la promenade de François ».</title></item>
        <item><persName ref="#le">LE</persName> : Projet : <title>« les temps emmêlés »</title></item>
        <item><persName ref="#pb">PB</persName> : Le principe de l’avalanche</item>
      </list>
      <p><label type="rubrique">Érudition :</label></p>
      <list type="unordered">
        <item><persName ref="#pf">PF</persName> : sortie de <title>l’anthologie des GR</title> de <persName>P Zumthor</persName></item>
        <item><persName ref="#pf">PF</persName> : les livres de <persName ref="#jr">Roubaud</persName></item>
        <item><persName ref="#jd">JD</persName> : un auteur à signaler.</item>
      </list>
      <p><label type="rubrique">Traduction :</label></p>
      <list type="unordered">
        <item><persName ref="#mb">MB</persName> : lettre et article du traducteur</item>
      </list>
      <p><label type="rubrique">Informations :</label></p>
      <list type="unordered">
        <item><persName ref="#fll">FLL</persName> : <persName>Andrée Vergès</persName>/ Film de vendredi</item>
      </list>
      <p><label type="rubrique">Prochaine réunion :</label></p>
      <p>10/8/78</p>
      <p>14/9.78</p>
      <p><label type="rubrique">Problème de logique :</label></p>
      <list type="unordered">
        <item><persName ref="#le">L.E.</persName></item>
      </list>
