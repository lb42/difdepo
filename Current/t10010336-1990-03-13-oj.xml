      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b100103361/f8.highres" n="8"/>
      <p rend="incipit">Réunion OULIPO</p>
      <p rend="incipit">Du 13 mars 1990</p>
      <p rend="incipit">Excusés : <persName ref="#jd">Jacques Duchateau</persName>, <persName ref="#jb">Jacques Bens</persName></p>
      <p rend="incipit">Absents : <persName ref="#jl">Lescure</persName>, <persName ref="#ab">Blavier</persName>, <persName ref="#rosensthiel">Rosensthiel</persName></p>
      <p rend="incipit">Présents : <persName ref="#mm">Michèle Métail</persName>, <persName ref="#mb"><choice><abbr>Marcel</abbr><expan>Marcel Bénabou</expan></choice></persName>, <persName ref="#pf">Paul Fournel</persName>, <persName ref="#fc">Caradec</persName>, <persName ref="#hm">Harry Mathews</persName>, <persName ref="#na"><choice><abbr>NA</abbr><expan>Noël Arnaud</expan></choice></persName></p>
      <p><label type="rubrique">Création</label></p>
      <list type="unordered">
        <item><persName ref="#fc"><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> : La <term ref="#concordancedutemps">concordance du temps</term></item>
      </list>
      <p><label type="rubrique">Erudition</label></p>
      <list type="unordered">
        <item><persName ref="#fc"><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> : Le <term ref="#romannecrologique">roman nécrologique</term></item>
        <item><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> :</item>
        <item><persName ref="#hm"><choice><abbr>HM</abbr><expan>Harry Mathews</expan></choice></persName> : <title ref="#viedelycurgue">Vie de Lycurgue</title></item>
      </list>
      <p><label type="rubrique">Action</label></p>
      <p><persName ref="#pf"><choice><abbr>PF</abbr><expan>Paul Fournel</expan></choice></persName> :</p>
      <list type="unordered">
        <item><title ref="#bibliothequeoulipienne">Bibliothèque oulipienne</title> résultats</item>
        <item>Action lectures</item>
        <item>Photographies</item>
        <item>Droits d’auteurs</item>
      </list>
      <p><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> :</p>
      <list type="unordered">
        <item><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> <persName ref="#fc"><choice><abbr>FC</abbr><expan>François Caradec</expan></choice></persName> <name ref="#thionville" type="manif">Thionville</name></item>
        <item><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> <name ref="#limoges" type="manif">Limoges</name></item>
        <item><persName ref="#mb"><choice><abbr>MB</abbr><expan>Marcel Bénabou</expan></choice></persName> Architecture</item>
        <item><orgName ref="#imec">IMEC</orgName></item>
      </list>
      <p>Desserts <unclear>pour pers.</unclear></p>
