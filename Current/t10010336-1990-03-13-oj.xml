      <p rend="incipit">Présents : <persName ref="difdepo:persons.xml#michelemetail">Michèle Métail</persName>, <persName ref="difdepo:persons.xml#marcel"><abbr>Marcel</abbr></persName>, <persName ref="difdepo:persons.xml#paulfournel">Paul Fournel</persName>, <persName ref="difdepo:persons.xml#caradec">Caradec</persName>, <persName ref="difdepo:persons.xml#harrymathews">Harry Mathews</persName>, <persName ref="difdepo:persons.xml#na"><abbr>NA</abbr></persName></p>
      <p><label type="rubrique">Création</label></p>
      <list type="unordered">
        <item><persName ref="difdepo:persons.xml#fc"><abbr>FC</abbr></persName> : La <term ref="difdepo:terms.xml#concordancedutemps">concordance du temps</term></item>
      </list>
      <p><label type="rubrique">Erudition</label></p>
      <list type="unordered">
        <item><persName ref="difdepo:persons.xml#fc"><abbr>FC</abbr></persName> : Le <term ref="difdepo:terms.xml#romannecrologique">roman nécrologique</term></item>
        <item><persName ref="difdepo:persons.xml#mb"><abbr>MB</abbr></persName> :</item>
        <item><persName ref="difdepo:persons.xml#hm"><abbr>HM</abbr></persName> : <title ref="difdepo:titles.xml#viedelycurgue">Vie de Lycurgue</title></item>
      </list>
      <p><label type="rubrique">Action</label></p>
      <p><persName ref="difdepo:persons.xml#pf"><abbr>PF</abbr></persName> :</p>
      <list type="unordered">
        <item><title ref="difdepo:titles.xml#bibliothequeoulipienne">Bibliothèque oulipienne</title> résultats</item>
        <item>Action lectures</item>
        <item>Photographies</item>
        <item>Droits d’auteurs</item>
      </list>
      <p><persName ref="difdepo:persons.xml#mb"><abbr>MB</abbr></persName> :</p>
      <list type="unordered">
        <item><persName ref="difdepo:persons.xml#mb"><abbr>MB</abbr></persName> <persName ref="difdepo:persons.xml#fc"><abbr>FC</abbr></persName> <name ref="difdepo:events.xml#thionville" type="manif">Thionville</name></item>
        <item><persName ref="difdepo:persons.xml#mb"><abbr>MB</abbr></persName> <name ref="difdepo:events.xml#limoges" type="manif">Limoges</name></item>
        <item><persName ref="difdepo:persons.xml#mb"><abbr>MB</abbr></persName> Architecture</item>
        <item><orgName ref="difdepo:organizations.xml#imec">IMEC</orgName></item>
