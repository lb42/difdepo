        <item><persName ref="difdepo:persons.xml#pb"><abbr>PB</abbr></persName> <term ref="difdepo:terms.xml#ontonymie">ontonymie</term></item>
        <item><persName ref="difdepo:persons.xml#mb"><abbr>MB</abbr></persName> <title ref="difdepo:titles.xml#silamour">Si l’amour</title></item>
        <item><persName ref="difdepo:persons.xml#fc"><abbr>FC</abbr></persName> Dictée de <term ref="difdepo:terms.xml#rimessinges">rimes singes</term></item>
        <item><persName ref="difdepo:persons.xml#na"><abbr>NA</abbr></persName> <term ref="difdepo:terms.xml#alexandrin">Style alexandrin</term></item>
      </list>
