while (<>) {
if (/Final\/([^:]+)/) {
   $invalid=$1;
   unless ($sick{$invalid}) {
          $sick{$invalid}=1;
	  $invalids++;
	  } else {
	  $sick{$invalid}++;
	  }
   }
else {
     $well++;
     }
}

    print $invalids, " of ", $invalids+$well, " had 1 or more errors\n";


    for $f (sort keys %sick) {
    $errs=$sick{$f};
    print "0" if ($errs < 10); 
    print "$errs $f \n";
    }
