      <pb facs="http://gallica.bnf.fr/ark:/12148/btv1b100103416/f5.highres" n="5"/>
      <p rend="incipit">24/08/90</p>
      <p rend="incipit">Présents : <persName ref="../thesauri/persons.xml#mb"><abbr>MB</abbr></persName>, <persName ref="../thesauri/persons.xml#jj"><abbr>JJ</abbr></persName>, <persName ref="../thesauri/persons.xml#hm"><abbr>HM</abbr></persName>, <persName ref="../thesauri/persons.xml#fc"><abbr>FC</abbr></persName>, <persName ref="../thesauri/persons.xml#jr"><abbr>JR</abbr></persName></p>
      <p rend="incipit">excusés : <persName ref="../thesauri/persons.xml#pf"><abbr>PF</abbr></persName>, <persName ref="../thesauri/persons.xml#na"><abbr>NA</abbr></persName>, <persName ref="../thesauri/persons.xml#pb"><abbr>PB</abbr></persName>.</p>
      <p><label type="rubrique">Création</label></p>
      <p><persName ref="../thesauri/persons.xml#hm"><abbr>HM</abbr></persName> nouvelle sous rubrique DENU</p>
      <p><persName ref="../thesauri/persons.xml#mb"><abbr>MB</abbr></persName> nouveau <term ref="../thesauri/terms.xml#croisement">croisement</term> : <term ref="../thesauri/terms.xml#langagecuitmoraleelementaire">langage cuit, morale élémentaire</term></p>
      <p><persName ref="../thesauri/persons.xml#mm"><abbr>MM</abbr></persName> réponse aux réunions de l’Oulipo</p>
      <p><label type="rubrique">Erudition</label></p>
      <p><persName ref="../thesauri/persons.xml#mb"><abbr>MB</abbr></persName> un nouveau concept oulipien : <term ref="../thesauri/terms.xml#plagiatparanticipationcanadadry">plagiat par anticipation Canada dry</term>.</p>
      <p><persName ref="../thesauri/persons.xml#fc"><abbr>FC</abbr></persName> <hi rend="sup">1</hi> maladie de l’<term ref="../thesauri/terms.xml#alexandrin">alexandrin</term></p>
      <p><persName ref="../thesauri/persons.xml#fc"><abbr>FC</abbr></persName> <hi rend="sup">2</hi> <term ref="../thesauri/terms.xml#xprendypourz">x prend y pour z</term></p>
      <p><persName ref="../thesauri/persons.xml#fc"><abbr>FC</abbr></persName> <hi rend="sup">3</hi> <title ref="../thesauri/titles.xml#pourphoebe">pour Phoebé</title></p>
      <p><persName ref="../thesauri/persons.xml#fc"><abbr>FC</abbr></persName> <hi rend="sup">4</hi> qu’importe le flacon pourvu qu’on ait l’étiquette</p>
      <p><persName ref="../thesauri/persons.xml#hm"><abbr>HM</abbr></persName> <hi rend="sup">1</hi> <title ref="../thesauri/titles.xml#entreneigeetglace">entre neige et glace</title></p>
      <p><persName ref="../thesauri/persons.xml#hm"><abbr>HM</abbr></persName> <hi rend="sup">2</hi> contre la <term ref="../thesauri/terms.xml#sextine">sextine</term></p>
      <p><persName ref="../thesauri/persons.xml#hm"><abbr>HM</abbr></persName> <hi rend="sup">3</hi> <term ref="../thesauri/terms.xml#s7">S+7</term> <persName ref="../thesauri/persons.xml#gilbertsorrentino">Gilbert Sorrentino</persName></p>
      <p><persName ref="../thesauri/persons.xml#jj"><abbr>JJ</abbr></persName> <hi rend="sup">1</hi> <term ref="../thesauri/terms.xml#plagiatparanticipation">plagiat par anticipation</term> de <term ref="../thesauri/terms.xml#polymorphie">polymorphie</term></p>
      <p><persName ref="../thesauri/persons.xml#jj"><abbr>JJ</abbr></persName> <hi rend="sup">2</hi></p>
      <p><title ref="../thesauri/titles.xml#bibliothequeoulipienne">Bibliothèque oulipienne</title> <persName ref="../thesauri/persons.xml#mb"><abbr>MB</abbr></persName> projets <term ref="../thesauri/terms.xml#poemescombinatoires">poèmes combinatoires</term></p>
      <p><label type="rubrique">Action</label></p>
      <p><persName ref="../thesauri/persons.xml#mb"><abbr>MB</abbr></persName> proposition d’un dictionnaire</p>
      <p><persName ref="../thesauri/persons.xml#fc"><abbr>FC</abbr></persName> <hi rend="sup">1</hi> Arsenal</p>
      <p><persName ref="../thesauri/persons.xml#fc"><abbr>FC</abbr></persName> <hi rend="sup">2</hi> Thionville</p>
      <p>Tous : discussion nouveaux membres</p>
      <p><label type="rubrique">Menu Propos</label></p>
      <p><persName ref="../thesauri/persons.xml#hm"><abbr>HM</abbr></persName> <hi rend="sup">1</hi> particule à <persName ref="../thesauri/persons.xml#hm"><abbr>HM</abbr></persName></p>
      <p><persName ref="../thesauri/persons.xml#hm"><abbr>HM</abbr></persName> <hi rend="sup">2</hi> examen oculaire du vieux salaud</p>
      <p><persName ref="../thesauri/persons.xml#mb"><abbr>MB</abbr></persName> lettre de <persName ref="../thesauri/persons.xml#francineperceval">Francine Perceval</persName></p>
      <p><persName ref="../thesauri/persons.xml#jj"><abbr>JJ</abbr></persName> <title ref="../thesauri/titles.xml#manufacture">Manufacture</title> (suite)</p>
