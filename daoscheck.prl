open (D, "filenames"); 
while (<D>) {
#print;
if (/T1001(\d\d\d\d)([\d\-]+)(..)\.xml/) {
   $idMap{$1} .= "$3 ";
   $n++;
}}
close D;
open (D, "daos.txt");
while (<D>) {
if (/btv1b1001(\d\d\d\d)(.)/) {
$found = $idMap{$1};
if ($found) {
   print "<keyVal><key>",$1,"<\/key><val>",$2,"<\/val><\/keyVal>\n";
   $nn++;
}
}}
print "$n docs found\n"; 
print "$nn with daos\n";


